println "arithmetic.uslang"

# addition
@a = 10, @b = 20
@c = @a + @b
println "${@a} + ${@b} = ${@c}"

# subtraction
@a = 10, @b = 20
@c = @a - @b
println "${@a} - ${@b} = ${@c}"

# multiplication
@a = 10, @b = 20
@c = @a * @b
println "${@a} * ${@b} = ${@c}"

# division
@a = 10.0, @b = 20
@c = @a / @b
println "${@a} / ${@b} = ${@c}"

# complex expression
@a = 10.0, @b = 20.0, @c = (((@a * 2) / @b) + 9)
println "((${@a} * 2) / ${@b}) + 9 = ${@c}"