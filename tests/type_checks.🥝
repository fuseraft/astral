@s = "kiwi"
@i = 100
@d = 3.14159
@b = true

class TypeCheckMe
  def initialize()
  end
end

@inst = TypeCheckMe.new()

@failed = 0

if !@s.is_a(String)
  @failed += 1
end

if !@i.is_a(Integer)
  @failed += 1
end

if !@d.is_a(Double)
  @failed += 1
end

if !@b.is_a(Boolean)
  @failed += 1
end

if !@inst.is_a(TypeCheckMe)
  @failed += 1
end

if !@inst.is_a(Object)
  @failed += 1
end

if @failed > 0
  println "Test(s) failed: ${@failed}"
else
  println "All tests passed!"
end

println ""