println "conditionals.uslang"

@a = true, @b = true

if @a == @b
    println "foo"
elsif @a != @b
    println "bar"
else
    println "baz"
endif

@a = true, @b = false

if @a == @b
    println "foo"
elsif @a != @b
    println "bar"
else
    println "baz"
endif

@a = true, @b = false

if @a == @b
    println "foo"
elsif @b == @a
    println "bar"
else
    println "baz"
endif
